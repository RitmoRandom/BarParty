<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="BarParty.Views.sizesNew"
             Title="Medida">
    <ScrollView>
        <VerticalStackLayout>
            <StackLayout>
                <Button x:Name="guardar" Text="Guardar" Clicked="Save" VerticalOptions="Start" HorizontalOptions="End" Margin="0,5,10,20" FontSize="20"/>
                <Entry x:Name="Nombre" Placeholder="Nombre" VerticalOptions="CenterAndExpand" HorizontalOptions="Center" FontSize="20"/>

            </StackLayout>
        </VerticalStackLayout>
    </ScrollView>
</ContentPage>